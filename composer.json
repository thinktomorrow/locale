{
    "name": "thinktomorrow/locale",
    "description": "A Laravel package for scalable route localization",
    "keywords": [
        "thinktomorrow",
        "locale"
    ],
    "homepage": "https://github.com/thinktomorrow/locale",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "cavensben@gmail.com"
        }
    ], 
    "require": {
        "php" : ">=7.1.8",
        "laravel/framework": "~5.6.0|~5.7.0|~5.8.0",
        "thinktomorrow/url": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "8.*",
        "mockery/mockery" : "~0.9|~1.0",
        "orchestra/testbench": "~3.6|~3.7|~3.8"
    },
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\Locale\\": "src/"
        },
      "classmap":[]
    },
  "autoload-dev": {
    "psr-4": {
      "Thinktomorrow\\Locale\\Tests\\": "tests"
    },
    "classmap":[]
  },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
              "Thinktomorrow\\Locale\\LocaleServiceProvider"
            ]
        }
    }
}
